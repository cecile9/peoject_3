            <div class="container rounded bg-white mt-5">
                <div class="row">
                    <div class="col-md-4 border-right">
                        <div class="d-flex flex-column align-items-center text-center p-3 py-5"><img class="rounded-circle mt-5" src="IMG_20201116_110201.jpg" width="90"><span class="font-weight-bold">Cécile Evezi</span><span class="text-black-50">CecilEvezi@bbb.com</span><span>United States</span></div>
                    </div>
                    <div class="col-md-8">
                        <div class="p-3 py-5">
                            <div class="d-flex justify-content-between align-items-center mb-3">
                                <div class="d-flex flex-row align-items-center back"><i class="fa fa-long-arrow-left mr-1 mb-1"></i>
                                    <h6>Back to home</h6>
                                </div>
                                <h6 class="text-right">Edit Profile</h6>
                            </div>
                            <div class="row mt-2">
                                <div class="col-md-6"><input type="text" class="form-control" placeholder="first name" value="NOM">NOM</div>
                                <div class="col-md-6"><input type="text" class="form-control" value="PRENOM" placeholder="PRENOM"></div>
                            </div>
                            <div class="row mt-3">
                                <div class="col-md-6"><input type="text" class="form-control" placeholder="Email" value="PROFESSION"></div>
                                <div class="col-md-6"><input type="text" class="form-control" value="063321222324" placeholder="NUMERO DE TELEPHONE"></div>
                            </div>
                            <div class="row mt-3">
                                <div class="col-md-6"><input type="text" class="form-control" placeholder="address" value="9 rue desbassins de richemond 92150 Suresnes"></div>
                                <div class="col-md-6"><input type="text" class="form-control" value="France" placeholder="Country"></div>
                            </div>
                            <div class="row mt-3">
                                <div class="col-md-6"><input type="text" class="form-control" placeholder="NOM DE L'ENTREPRISE" value="Calendify"></div>
                                <div class="col-md-6"><input type="text" class="form-control" value="ADMINISTRATEUR" placeholder="Account Number"></div>
                            </div>
                            <div class="mt-5 text-right"><button class="btn btn-primary profile-button" type="button">Save Profile</button></div>
                        </div>
                    </div>
                </div>
            </div>
