{% endblock %}
    <style>
        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            user-select: none;
        }

        @media (min-width: 768px) {
            .bd-placeholder-img-lg {
                font-size: 3.5rem;
            }
        }

        #navbar {
            background-color: darksalmon;
        }
    </style>
</head>
<body>

<!--NAV-->

<header class="p-3 bg-default text-white" id="navbar">
    <div class="container-fluid">
        <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
            <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
                <svg class="bi me-2" width="40" height="32"><use xlink:href="#bootstrap"/></svg>
            </a>

            <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                <li><a href="/" class="nav-link px-2 text-white">ACCUEIL</a></li>
                <li><a href="{{ path('contact_index') }}" class="nav-link px-2 text-white">CALENDRIER</a></li>
                <li><a href="{{ path('user-calendar-dashboard') }}" class="nav-link px-2 text-white">DASHBOARD</a></li>
                <li><a href="{{ path('contact_index') }}" class="nav-link px-2 text-white">CONTACTS</a></li>
            </ul>
            <div class="text-end">
